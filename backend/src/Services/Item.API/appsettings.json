{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=MarketplaceDb;Username=postgres;Password=postgres"
  },
  "Authentication": {
    "Authority": "https://localhost:8080/realms/marketplace",
    "Audience": "marketplace-api",
    "Issuer": "marketplace-api",
    "SecretKey": ""
  },
  "Caching": {
    "DefaultExpirationMinutes": 10,
    "SlidingExpirationMinutes": 2
  },
  "AllowedOrigins": ["https://localhost:3000", "http://localhost:3000"],
  "Pagination": {
    "MaxPageSize": 100,
    "DefaultPageSize": 10
  },
  "AllowedHosts": "*"
}